{
  "compilerOptions": {
    "target": "es2023",
    "module": "nodenext",
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "resolvePackageJsonImports": true,
    "noImplicitAny": false
  },
  "include": ["src"],
  "exclude": ["node_modules","dist"]
}
